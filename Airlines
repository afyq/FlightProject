    public static void booking(){
        Scanner s = new Scanner(System.in);
        System.out.println("\nPlease fill in the following contact details;");
        System.out.print("Enter your full name: ");
        fullname = s.nextLine();
        System.out.print("Enter your email id: ");
        email = s.nextLine();
        System.out.print("Enter your phone number (10 digit): ");
        phone = s.nextLine();
        System.out.println();
    }
    public static String date(){
        Scanner s = new Scanner(System.in);
        System.out.println("Available Date;\n1. 18 Jun\n2. 20 Jun\n3. 22 Jun");
        System.out.print("Enter the date: ");
        choiceDate = s.nextInt();
        if (choiceDate == 1){
            selectedDates = dates[0];
        }
        else if (choiceDate == 2){
            selectedDates = dates[1];        }
        else{
            selectedDates = dates[2];
        }
        return selectedDates;
    }
    public static int taxes(int total){   
        Scanner s = new Scanner(System.in);
        System.out.println("Seats Available;" + '\n' + "1. First class" + '\n' + "2. Economy");
        System.out.print("Enter category of seat booked: ");
        choice3 = s.nextInt();
        if (choice3 == 1){
            tax = (int)((20/100.0)*total);
        }
        else {
            tax = (int)((5/100.0)*total);
        }
        finalprice = (int) (total + tax);
        System.out.println("\nAirline taxes   = RM "+tax);
        System.out.println("Your final bill = RM "+ (total+tax));
        System.out.println();
        return (finalprice);
    }
    public static void seats()throws IOException{
            BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
            String full = "\u2612"; //unicode character('Ballot box with X')- U+2612
            String empty = "\u25A1"; //unicode character('White Square')- U+25A1
            int i;    
            System.out.println("");
            System.out.print("      ");
            for (i=1; i<=6 ; i++){
                    if(i==4)
                    System.out.print("         ");
                    System.out.print(i);
                    System.out.print("     ");
            }
            System.out.println();
            for (i = 65; i<=73; i++){ //unicode decimal 65-73 = A-I
                if (i==65)//'A'
                System.out.println((char)i +"     "+ empty +"    "+ empty +"     "+ empty +"              "+ empty +"    "+ empty+"     "+ empty +"     " + (char)i);
                else if(i==66)//'B'
                System.out.println((char)i +"     "+ empty +"    "+ empty +"     "+ empty +"              "+ empty +"    "+ empty+"     "+ empty +"     " + (char)i);
                else if(i==67)//'C'
                System.out.println((char)i +"     "+ empty +"    "+ empty +"     "+ empty +"              "+ empty +"    "+ empty+"     "+ empty +"     " + (char)i);
                else if(i==68)//'D'
                System.out.println((char)i +"     "+ empty +"    "+ empty +"     "+ empty +"              "+ empty +"    "+ empty+"     "+ empty +"     " + (char)i);
                else if(i==69)//'E'
                    {
                        System.out.println();
                        System.out.println();
                    }
                else if(i==70)//'F'
                System.out.println((char)i +"     "+ empty +"    "+ empty +"     "+ empty +"              "+ empty +"    "+ empty+"     "+ empty +"     " + (char)i);
                else if(i==71)//'G'
                System.out.println((char)i +"     "+ empty +"    "+ empty +"     "+ empty +"              "+ empty +"    "+ empty+"     "+ empty +"     " + (char)i);
                else if(i==72)//'H'
                System.out.println((char)i +"     "+ empty +"    "+ empty +"     "+ empty +"              "+ empty +"    "+ empty+"     "+ empty +"     " + (char)i);
                else if(i==73)//'I'
                System.out.println((char)i +"     "+ empty +"    "+ empty +"     "+ empty +"              "+ empty +"    "+ empty+"     "+ empty +"     " + (char)i);
            }
            String s[] = new String[no];
            //Scanner input = new Scanner(System.in);
            for(i=0;i<no;i++){
                System.out.println("\nEnter seat address:");
                s[i] = br.readLine(); 
                for(int j=0;j<no;j++){
                    if(s[i]==ss[j]){   
                        ss[j] = s[i];
                    }
                }
            }
            //System.out.println("");
            System.out.println("These are your selected seats:");
            for(i=0;i<no;i++){
                System.out.println(s[i]);
            }
            System.out.println("");
        }
}
